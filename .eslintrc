{
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "atom": true
  },
  "settings": {
    "import/core-modules": ["atom"]
  },
  "rules": {
    "prettier/prettier": ["error", {"bracketSpacing": false}],
    "strict": 0,
    "consistent-return": 2,
    "curly": ["error", "multi-line"],
    "handle-callback-err": 2,
    "no-caller": 2,
    "no-console": 1,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-else-return": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-mixed-requires": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-param-reassign": [
      2,
      {
        "props": true
      }
    ],
    "no-path-concat": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-sync": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-unexpected-multiline": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [
      2,
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-var": 2,
    "no-void": 2,
    "no-with": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-spread": 2,
    "prefer-reflect": 1,
    "prefer-template": 2,
    "radix": 2,
    "require-yield": 2,
    "valid-jsdoc": 0,
    "yoda": [
      2,
      "never"
    ],
    "camelcase": 2,
    "func-style": 0,
    "linebreak-style": 2,
    "max-nested-callbacks": 0,
    "new-cap": 2,
    "no-lonely-if": 2,
    "no-nested-ternary": 0,
    "no-spaced-func": 2,
    "no-underscore-dangle": 2,
    "no-unneeded-ternary": 2,
    "spaced-comment": 2
  }
}
